{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["@typescript-eslint/eslint-plugin"],
  "rules": {
    
    "prettier/prettier": "error",
    "no-unused-vars": "error",
    "prefer-const": "error",
    "no-unused-expressions": "error",
    "no-undef": "error",
    "no-console": "warn"
  }
}
